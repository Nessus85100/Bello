<?xml version="1.0" encoding="UTF-8"?>
<includes> 

  <!-- TV Guide Layout -->
	<include name="TVGuideLayout">
		<itemlayout width="160" height="84">
			<control type="image">
				<description>Banner</description>
				<left>0</left>
				<top>0</top>
				<width>160</width>
				<height>90</height>
				<aspectratio aligny="top" align="center">keep</aspectratio>
				<texture background="true">$INFO[ListItem.Property(Art(banner))]</texture>
				<bordertexture border="4">dialogs/other/iconborder.png</bordertexture>
				<bordersize>4</bordersize>
			</control>
			<control type="label">
				<description>Episode Number</description>
				<left>0</left>
				<top>29</top>
				<width>150</width>
				<height>35</height>
				<label>$INFO[ListItem.Property(NextNumber),$LOCALIZE[20359] ]</label>
				<textcolor>77white</textcolor>
				<font>FontSize_16</font>
				<align>center</align>
			</control>
			<control type="label">
				<description>Next air date/time</description>
				<left>0</left>
				<top>49</top>
				<width>150</width>
				<height>35</height>
				<label>$INFO[ListItem.Property(NextDate)]$INFO[ListItem.Property(ShortTime), (,)]</label>
				<textcolor>77white</textcolor>
				<font>FontSize_16</font>
				<align>center</align>
			</control>
		</itemlayout>
		<focusedlayout width="160" height="84">
			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>160</width>
				<height>80</height>
				<texture border="20">buttons/focus.png</texture>
	      <include>VisibleFadeEffect2</include>
			</control>
			<control type="image">
				<description>Banner</description>
				<left>0</left>
				<top>0</top>
				<width>160</width>
				<height>90</height>
				<aspectratio aligny="top" align="center">keep</aspectratio>
				<texture background="true">$INFO[ListItem.Property(Art(banner))]</texture>
				<bordertexture border="4">dialogs/other/iconborder.png</bordertexture>
				<bordersize>4</bordersize>
			</control>
			<control type="label">
				<description>Episode Number</description>
				<left>0</left>
				<top>29</top>
				<width>150</width>
				<height>35</height>
				<label>$INFO[ListItem.Property(NextNumber),$LOCALIZE[20359] ]</label>
				<textcolor>white</textcolor>
				<font>FontSize_16</font>
				<align>center</align>
			</control>
			<control type="label">
				<description>Next air date/time</description>
				<left>0</left>
				<top>49</top>
				<width>150</width>
				<height>35</height>
				<label>$INFO[ListItem.Property(NextDate)]$INFO[ListItem.Property(ShortTime), (,)]</label>
				<textcolor>white</textcolor>
				<font>FontSize_16</font>
				<align>center</align>
			</control>
		</focusedlayout>
	</include>

